{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "expect": true,
    "it": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "document": false,
    "navigator": false,
    "window": false
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "arrow-parens": 0
  }
}
